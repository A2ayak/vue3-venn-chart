{
	"name": "vue3-venn-chart",
	"private": false,
	"version": "1.0.1",
	"description": "venn chart for Vue3",
	"author": "Azayak",
	"keywords": [
		"venn",
		"venn chart",
		"venn-chart",
		"vue3-venn-chart",
		"vue3",
		"venn.js",
		"d3.js"
	],
	"type": "module",
	"scripts": {
		"dev": "vite --port 4090",
		"build": "vite build",
		"preview": "vite preview"
	},
	"main": "./lib/vue3-venn-chart.js",
	"module": "./lib/vue3-venn-chart.js",
	"build": {
		"cssCodeSplit": true,
		"lib": {
			"format": [
				"es"
			]
		}
	},
	"exports": {
		".": {
			"import": "./lib/vue3-venn-chart.js",
			"require": "./lib/vue3-venn-chart.umd.cjs"
		}
	},
	"dependencies": {
		"@types/lodash-es": "^4.17.7",
		"@upsetjs/venn.js": "^1.4.2",
		"d3": "^5.9.7",
		"lodash-es": "^4.17.21",
		"vue": "^3.2.47"
	},
	"devDependencies": {
		"@types/node": "^18.15.11",
		"@vitejs/plugin-vue": "^4.1.0",
		"less": "^4.1.3",
		"typescript": "^4.9.3",
		"vite": "^4.2.0",
		"vite-plugin-css-injected-by-js": "^3.1.0",
		"vite-plugin-vue-setup-extend": "^0.4.0",
		"vue-tsc": "^1.2.0"
	}
}
